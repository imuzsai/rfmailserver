user = {{ postfix_dovecot_mysql_user }}
password = {{ postfix_dovecot_mysql_password }}
hosts = {{ postfix_dovecot_mysql_host }}
dbname = {{ postfix_dovecot_mysql_db_name }}
query = SELECT goto FROM alias,alias_domain
  WHERE alias_domain.alias_domain = '%d'
  AND alias.address=concat('%u', '@', alias_domain.target_domain)
  AND alias.active = 1
